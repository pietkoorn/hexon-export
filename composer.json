{
    "name": "pietkoorn/hexon-export",
    "description": "A Laravel 5.5+ package to process imcremental XML exports from Hexon's Doorlinken Voorraad. Fork by Piet Koorn",
    "version": "0.1.2",
    "keywords": [
        "currenlty open",
        "laravel"
    ],
    "homepage": "https://github.com/pietkoorn/hexon-export",
    "authors": [
    {
        "name": "Piet Koorn",
        "email": "piet@goforit.nl"
    }
    ],
    "require": {
        "php": ">=7.0",
        "nesbot/carbon": "~2.28"
    },
    "require-dev": {
        "illuminate/framework": "5.5.x"
    },
    "autoload": {
        "psr-4": {
            "RoyScheepens\\HexonExport\\": "src/"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "RoyScheepens\\HexonExport\\HexonExportServiceProvider"
            ],
            "aliases": {
                "HexonExport": "RoyScheepens\\HexonExport\\HexonExportFacade"
            }
        }
    }
}
